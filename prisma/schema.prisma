generator client {
  provider = "prisma-client-js"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model StartupAnalysis {
  id             String   @id @default(cuid())
  startupName    String
  pitch          String
  revenueModel   String
  industry       String
  stage          String
  description    String
  problem        String
  result         String
  score          Int
  keyImprovements String?
  createdAt      DateTime @default(now())
}